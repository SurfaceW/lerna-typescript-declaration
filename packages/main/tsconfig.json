{
"compilerOptions": {
  "lib": ["ES2015", "DOM"],
  "module": "CommonJS",
  "noImplicitAny": true,
  "removeComments": true,
  "preserveConstEnums": true,
  "moduleResolution": "Node",
  "outDir": "./demo/dist",
  "target": "ES2015",
  "types": ["typings/*.d.ts"]
},
"include": ["src/**/*", "typings/*.d.ts"],
"exclude": ["node_modules", "**/*.spec.ts"]
}
